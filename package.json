{
  "private": true,
  "dependencies": {
    "@faithlife/flcom-addin-scripts": "4.10.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "workspaces": [
    "packages/*",
    "examples/*"
  ],
  "license": "UNLICENSED",
  "scripts": {
    "start": "flcom-addin-scripts start",
    "build": "flcom-addin-scripts build",
    "create-package": "flcom-addin-scripts create-package",
    "create-example": "flcom-addin-scripts create-example",
    "clean": "rm -rf packages/*/dist",
    "lint": "flcom-addin-scripts lint",
    "ship": "flcom-addin-scripts ship",
    "ship-to": "flcom-addin-scripts ship-to",
    "get-versions": "flcom-addin-scripts get-versions",
    "test": "lerna run test",
    "publish": "lerna publish --conventional-commits --github-release --registry http://npm.faithlife.io --yes"
  },
  "browserslist": [
    "extends @faithlife/browserslist-config-flcom-addin"
  ],
  "eslintConfig": {
    "root": true,
    "extends": "@faithlife/eslint-config-flcom-addin",
    "overrides": [
      {
        "files": [
          "examples/**"
        ],
        "rules": {
          "react/jsx-no-literals": "off"
        }
      }
    ]
  },
  "eslintIgnore": [
    "**/dist",
    "**/node_modules"
  ],
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "commitlint-config-faithlife": "^1.0.0",
    "husky": "^3.1.0",
    "lerna": "^3.19.0",
    "typescript": "^3.7.2"
  },
  "resolutions": {
    "@babel/runtime": "7.3.1",
    "@types/react": "^16.9.11",
    "@emotion/memoize": "0.7.2",
    "@emotion/unitless": "0.7.4",
    "@emotion/is-prop-valid": "0.8.5",
    "@faithlife/styled-ui": "^5.36.0",
    "regenerator-runtime": "0.13.3",
    "styled-components": "4.4.1",
    "typescript": "^3.7.2",
    "lint-staged": "^8.1.5",
    "prop-types": "15.7.2",
    "react-is": "16.10.1",
    "sockjs-client": "1.4.0"
  },
  "name": "FaithlifeEquipment"
}
